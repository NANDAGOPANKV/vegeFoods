    <script>
var xValues = ["All Order", "All Delivered Order", "all Return Orders"];
let one = "{{allOrders}}" 
let two = "{{deliverdOrders}}"
let three = "{{allReturnOrders}}"

var yValues = [one, two, three,0];
var barColors = ["red", "green","blue","orange","brown"];

// c1
new Chart("myChart", {
  type: "bar",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "All Orders"
    }
  }
});

// c2
